# Proxy Rotation Service Configuration
# Copy this to .env and customize

# Database
DATABASE_PATH=proxies.db

# API Settings
API_HOST=0.0.0.0
API_PORT=5000
API_KEY=your-secret-api-key-here
DEBUG=False

# Rate Limiting
RATE_LIMIT_ENABLED=True
RATE_LIMIT_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO
LOG_FILE=proxy_service.log
